                 <table class="table table-striped table-primary">
                  <thead>
                    <tr>
                      <th>Teacher Image</th>
                      <th>Teacher Name</th>
                      <th>Teacher ID</th>
                
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody>
                   <% if(records.length >0){
                    records.forEach(function(row){
                    %>
                    <tr>
                      <td><img class="img2" src="./uploads/<%= row.image %>" width="50"></td>
                      <td><%= row.teachername %></td>
                      <td><%= row.teacher_uid %></td>
                       <td></td> <a class="btn btn-danger btn-xs" href="/teacherList/delete/<%= row._id%>"><i class="fa fa-trash"></i></a></td>
                    </tr>
                    <%  }) } %>
                  </tbody>
                </table>
              </div>
            </div>

              
    
